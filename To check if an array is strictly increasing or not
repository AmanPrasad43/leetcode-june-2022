import java.util.*;
public class Square{
	 public static boolean check(int A[],int idx)
	    {
	        //Your code here
	       if(idx==A.length-1) {
	    	   return true;
	       }
	       if(A[idx]>A[idx+1]) {
	    	   return false;
	       }
	       else
	       {
	    	   return check(A,idx+1);
	       }
	      
	    }
	        
	      
	public static void main(String args[]) {
		int[] aman = new int[] {1,2,4};
		
		boolean result = check(aman,0);
		
        System.out.println(result);

		
	}
}
